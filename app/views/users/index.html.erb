<h1>Users</h1>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user_path(user.id) %>
      <%= link_to "[edit]", edit_user_path(user.id) %>
      <%= link_to "[del]", user_path(user.id), method: :delete, data: { confirm: "are you sure?" } %>
    </li>
  <% end %>
</ul>

<p><%= link_to "Add New", new_user_path %></p>
<p><%= link_to "Collection Page", collection_path %></p>
